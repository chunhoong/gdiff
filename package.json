{
  "name": "gdiff",
  "version": "2.0.0-beta.1",
  "description": "Utility to detect changed files between 2 commits",
  "bin": {
    "gdiff": "./bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "jest": "jest --watch --verbose",
    "clean": "rimraf ./bin",
    "build": "npm run clean && tsc",
    "prepare": "husky install",
    "lint": "prettier --write .",
    "lint-staged": "pretty-quick --staged",
    "prepack": "rimraf ./gdiff.*.tgz && npm run build"
  },
  "keywords": [
    "git"
  ],
  "author": "Lim Chun Hoong",
  "license": "MIT",
  "homepage": "https://github.com/chunhoong/gdiff#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/chunhoong/gdiff.git"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/minimatch": "^3.0.4",
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^16.0.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "minimatch": "^3.0.4",
    "shelljs": "^0.8.4",
    "yargs": "^16.2.0"
  }
}
